MyCommand=${0##*"/"}
CDir=${0%%/"$MyCommand"}
ScriptDir=$(cd $CDir && pwd)
ScriptCommand=SAP.py

Version=0.70

python --version

if [ "$?" != "0" ]
then
	sudo apt-get install python
fi

clear

echo """
                         ###     StudioAndroid      ###
                         ###     GTK GUI Build      ###
                                      v$Version

"""

python $ScriptDir/$ScriptCommand






